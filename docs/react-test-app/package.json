{
  "name": "hello-world",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start"
  },
  "prettier": "@infotition/prettier-config",
  "eslintConfig": {
    "extends": [
      "@infotition/eslint-config"
    ],
    "ignorePatterns": [
      "*.js",
      "lib/**"
    ],
    "rules": {
      "import/no-default-export": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
