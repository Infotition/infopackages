{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "deploy": {
      "dependsOn": ["build", "lint"],
      "outputs": ["package"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test": { "outputs": [] },
    "lint": { "outputs": [] },
    "clean": { "outputs": [] }
  }
}
